<h1>Listing vote_items</h1>

<table>
  <thead>
    <tr>
      <th>Topic</th>
      <th>Project</th>
      <th>Vote</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vote_items.each do |vote_item| %>
      <tr>
        <td><%= vote_item.topic %></td>
        <td><%= vote_item.project %></td>
        <td><%= vote_item.vote %></td>
        <td><%= link_to 'Show', vote_item %></td>
        <td><%= link_to 'Edit', edit_vote_item_path(vote_item) %></td>
        <td><%= link_to 'Destroy', vote_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vote item', new_vote_item_path %>
